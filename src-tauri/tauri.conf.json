{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "zsbis",
  "version": "1.0.0",
  "identifier": "ZSBIS",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "ZSBIS",
        "width": 450,
        "height": 170,
        "resizable": false,
        "maximizable": false,
        "contentProtected": false,
        "center": true,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icon.ico"]
  }
}
